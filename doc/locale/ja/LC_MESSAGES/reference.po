msgstr ""

#: ../../reference/configuration/logs_xml.config.en.rst:49
msgid "The following list shows ``LogFormat`` specifications."
msgstr ""

#: ../../reference/configuration/logs_xml.config.en.rst:57
msgid "``<Name = \"valid_format_name\"/>``"
